#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/前端代码/练习项目/api_serve/node_modules/semver/bin/node_modules:/mnt/e/前端代码/练习项目/api_serve/node_modules/semver/node_modules:/mnt/e/前端代码/练习项目/api_serve/node_modules:/mnt/e/前端代码/练习项目/node_modules:/mnt/e/前端代码/node_modules:/mnt/e/node_modules:/mnt/e/前端代码/练习项目/api_serve/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/前端代码/练习项目/api_serve/node_modules/semver/bin/node_modules:/mnt/e/前端代码/练习项目/api_serve/node_modules/semver/node_modules:/mnt/e/前端代码/练习项目/api_serve/node_modules:/mnt/e/前端代码/练习项目/node_modules:/mnt/e/前端代码/node_modules:/mnt/e/node_modules:/mnt/e/前端代码/练习项目/api_serve/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../semver/bin/semver" "$@"
else
  exec node  "$basedir/../semver/bin/semver" "$@"
fi
